{
  "name": "@0x-jerry/imger",
  "description": "A command line tool that help you to generator images, support icns and ico.",
  "version": "0.3.0",
  "main": "./dist/export.js",
  "types": "./dist/export.d.ts",
  "type": "module",
  "license": "MIT",
  "bin": {
    "imger": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "dev": "tsx dev.ts",
    "build": "npm run build:cli && npm run build:lib",
    "build:cli": "tsup src/cli.ts --format esm",
    "build:lib": "tsup src/export.ts --format esm --dts",
    "release": "x-release",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "@0x-jerry/utils": "^2.1.1",
    "cac": "^6.7.14",
    "debug": "^4.3.4",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "picocolors": "^1.0.0",
    "png2icons": "^2.0.1",
    "prompts": "^2.4.2",
    "sharp": "^0.33.2",
    "unconfig": "^0.3.11"
  },
  "devDependencies": {
    "@0x-jerry/x-release": "^0.4.0",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.17",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}